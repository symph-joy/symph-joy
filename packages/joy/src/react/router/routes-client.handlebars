import {JoyReactRouteLoader} from '@symph/joy/dist/react/router/joy-react-route-loader'

{{#*inline "renderRoutes"}}
  [
  {{#children}}
    {
      path: "{{path}}",
      {{#if dynamic}}dynamic: {{json dynamic}},{{/if}}
      {{#if index}}index: {{json index}},{{/if}}
      {{#if caseSensitive}}caseSensitive: {{json caseSensitive}},{{/if}}
      {{#if catchAllParam}}catchAllParam: {{json catchAllParam}},{{/if}}
      componentName: "{{componentName}}",
      {{#if componentPackage}}componentPackage: "{{componentPackage}}",{{/if}}
      componentModule: {{#if dynamic}}()=>import({{json srcPath}}){{else}}require({{json srcPath}}){{/if}},
      render: (routeProps) => {
        return <JoyReactRouteLoader {...routeProps} />;
      },
    children: {{> renderRoutes this}}
    },
  {{/children}}
  ]
{{/inline}}

export const joyReactAutoGenRoutes = {
  name: "joyReactAutoGenRoutes",
  type: Array,
  useValue: {{> renderRoutes this }}
}
