import {JoyReactRouteLoader} from '@symph/joy/dist/react/router/joy-react-route-loader'

export const joyReactAutoGenRoutes = {
  name: "joyReactAutoGenRoutes",
  type: Array,
  useValue: [
    {{#routes}}
    {
      path: "{{path}}",
      dynamic: {{json dynamic}},
      providerName: "{{providerName}}",
      providerModule: {{#if dynamic}}()=>import({{json srcPath}}){{else}}require({{json srcPath}}){{/if}},
      render: (routeProps) => {
        return <JoyReactRouteLoader {...routeProps} />;
      },
      //component: {{#if dynamic}}()=>import({{json srcPath}}){{else}}require({{json srcPath}}){{/if}},
    },
    {{/routes}}
  ]
}
