import {JoyReactRouteLoader} from '@symph/joy/dist/react/router/joy-react-route-loader'
import {MountModule} from "@symph/react/dist/mount/mount-module";

{{#*inline "renderRoutes"}}
  [
  {{#routes}}
    {
    path: "{{path}}",
    {{#if exact}}exact: {{json exact}},{{/if}}
    {{#if sensitive}}sensitive: {{json sensitive}},{{/if}}
    {{#if strict}}strict: {{json strict}},{{/if}}
    {{#if dynamic}}dynamic: {{json dynamic}},{{/if}}
    providerName: "{{providerName}}",
    {{#if providerPackage}}providerPackage: "{{providerPackage}}",{{/if}}
    providerModule: {{#if mount}}{mount:"{{mount}}", module:require({{json srcPath}})}{{else}}require({{json srcPath}}){{/if}},
    routes: {{> renderRoutes this}}
    },
  {{/routes}}
  ]
{{/inline}}

export const joyReactAutoGenRoutes = {{> renderRoutes this}}
